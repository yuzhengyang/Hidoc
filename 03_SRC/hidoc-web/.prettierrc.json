{
    "singleQuote": true,
    "semi": true,
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 1200,
    "endOfLine": "auto",
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "eslintIntegration": true,
    "htmlWhitespaceSensitivity": "ignore",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    }
}
